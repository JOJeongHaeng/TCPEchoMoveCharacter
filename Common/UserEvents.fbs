namespace UserEvents;

struct Color { 
	r: uint8;
	g: uint8;
	b: uint8;
}

enum ChatType : byte {
	All = 0,
	Team = 1,
	Private = 2,
	System = 3
}

table C2S_Login {
	userid: string;
	password: string;
}

table S2C_Login {
	player_id: uint32;
	success: bool;
	message: string;
	x: uint16;
	y: uint16;
	color: Color;
}

union EventType {
	C2S_Login,
	S2C_Login,
}

table EventData {
	timestamp: uint64;
	data: EventType;
}

root_type EventData;